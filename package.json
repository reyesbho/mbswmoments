{
  "name": "mbswmoments",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start",
    "start:prod": "expo start --no-dev --minify",
    "reset-project": "node ./scripts/reset-project.js",
    "set-env:dev": "node ./scripts/set-env.js development",
    "set-env:preview": "node ./scripts/set-env.js preview",
    "set-env:prod": "node ./scripts/set-env.js production",
    "android": "expo start --android",
    "android:dev": "expo start --android",
    "android:prod": "expo start --android --no-dev --minify",
    "ios": "expo start --ios",
    "ios:dev": "expo start --ios",
    "ios:prod": "expo start --ios --no-dev --minify",
    "web": "expo start --web",
    "web:dev": "expo start --web",
    "web:prod": "expo start --web --no-dev --minify",
    "build:android": "expo run:android --no-dev --minify",
    "build:ios": "expo run:ios --no-dev --minify",
    "build:web": "expo export:web --no-dev --minify",
    "build:preview": "node scripts/prepare-build.js preview && npx eas build --platform android --profile preview",
    "build:production": "node scripts/prepare-build.js production && npx eas build --platform android --profile production",
    "check-env": "node scripts/check-env.js",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.2.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.84.0",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-device": "^7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.61.1",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
